    "babel-jest": "^25.0.0",
    "cypress": "^3.4.1",
    "eslint": "^6.4.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "husky": "^3.0.5",
    "jest": "^25.0.0",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.0",
    "vue-jest": "^4.0.0-0",
    "vue2-animate": "^2.1.2"
  }
